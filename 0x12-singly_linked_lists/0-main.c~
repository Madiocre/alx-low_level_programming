#include <stdio.h>
#include "your_list_t_header.h" // Assuming you have a header file for your list_t structure

/**
 * print_list - Prints all the elements of a list_t linked list.
 * @h: A pointer to the head of the linked list.
 *
 * Return: The number of nodes in the linked list.
 */
size_t print_list(const list_t *h)
{
    const list_t *current = h; // Start at the head of the linked list
    size_t count = 0; // Counter to keep track of the number of nodes

    while (current != NULL)
    {
        if (current->str != NULL)
        {
            // Print the content of the node
            printf("[%d] %s\n", count, current->str);
        }
        else
        {
            // Print [0] (nil) if str is NULL
            printf("[%d] (nil)\n", count);
        }

        current = current->next; // Move to the next node
        count++; // Increment the counter
    }

    return count;
}
